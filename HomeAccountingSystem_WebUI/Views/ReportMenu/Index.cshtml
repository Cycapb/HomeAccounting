@model HomeAccountingSystem_WebUI.Abstract.IReportMenu
@foreach (var item in Model.Items)
{
    if (item.Id != 3)
    {
        @Html.RouteLink(item.Name, new
        {
            controller = "Report",
            action = "CreateByTypeOfFlowView",
            id = item.Id
        }, new {@class = "btn btn-block btn-raised btn-default" + (item.Id == ViewBag.ReportMenuId ? " btn-info" : "")})
    }
    else
    {
        @Html.RouteLink(item.Name, new
        {
            controller = "Report",
            action = "CreateByDatesView",
            id = item.Id
        }, new {@class = "btn btn-block btn-raised btn-default" + (item.Id == ViewBag.ReportMenuId ? " btn-info" : "")})
    }
}
