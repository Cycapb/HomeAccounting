@model HomeAccountingSystem_WebUI.Models.BudgetModel
<div class="panel panel-success">
    <div class="panel-heading">@Model.TypeOfMoney</div>
    <table class="table table-striped">
        <tr>
            <th>Месяц</th>
            <td>@Model.Month.ToString("c")</td>
        </tr>
        <tr>
            <th>Неделя</th>
            <td>@Model.Week.ToString("c")</td>
        </tr>
        <tr>
            <th>День</th>
            <td>@Model.Day.ToString("c")</td>
        </tr>
    </table>
</div>
