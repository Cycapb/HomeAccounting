@using System.Web.Mvc.Html
@model LoginModel
@{
    Layout = "~/Views/Shared/_LoginLayout.cshtml";
}

<div class="container">
    <div class="row">
        <div class="col-md-4 col-md-offset-4 col-sm-6 col-sm-offset-3" id="loginFormContainer">
            @Html.Action("Login", new { returnUrl = ViewBag.returnUrl });
        </div>
    </div>
</div>