@model CategoriesCollectionModel
<table class="table table-striped">
    <thead>
        <tr>
            <th>
                Наименование
            </th>
            <th>
                Подкатегории
            </th>
            <th>
                Действия
            </th>
        </tr>
    </thead>
    <tbody id="categoryContainer">
        @await Html.PartialAsync("_CategorySummary", Model.Categories)
    </tbody>
</table>
<div class="text-center">
    @await Html.PartialAsync("_PageLinks", Model)
</div>