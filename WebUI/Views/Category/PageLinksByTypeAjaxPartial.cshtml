@model WebUI.Models.CategoryModels.CategoriesCollectionModel

@for (int i = 1; i <= Model.PagingInfo.TotalPages; i++)
{
    @Ajax.ActionLink(i.ToString(), "GetCategoriesByType", new {typeOfFlowId = Model.TypeOfFlowId, page = i},
        new AjaxOptions()
        {
            Url = Url.Action("GetCategoriesByType", new {typeOfFlowId = Model.TypeOfFlowId, page = i}),
            UpdateTargetId = "categoryContainer"
        }, new {@class = "btn btn-default btn-raised btn-round btn-sm", id=i.ToString(), onclick = "changeButtonColor(this.id)" })
}